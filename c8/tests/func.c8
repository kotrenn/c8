fun int square(int n)
  int ret = n * n;
  return ret;
nuf

fun int sum(int a, int b, int c)
  return a + b + c;
nuf

fun int max(int a, int b)
  if (a > b)
    return a;
  else
    return b;
  fi
nuf

fun int max3(int a, int b, int c)
  return max(a, max(b, c));
nuf

